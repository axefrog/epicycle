{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "semi": [2, "always"],
    "new-cap": 0,
    "strict": 0,
    "no-underscore-dangle": 0,
    "no-unused-vars": [1, {"vars": "all", "args": "none"}],
    "eol-last": 0,
    "no-undef": 2,
    "quotes": [2, "single", "avoid-escape"]
  }
}
